{
  "name": "n8n-nodes-nocodb-upsert",
  "version": "0.1.6",
  "description": "EXPERIMENTAL, do not use yet. NocoDb node for n8n with an Append/Update (upsert) action.",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package",
    "nocodb",
    "upsert"
  ],
  "author": {
    "name": "John Doe",
    "email": "me@me.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/john-doe/n8n-nodes-nocodb-upsert.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@10.8.1",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NocoDbUpsertApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NocoDbUpsert/NocoDbUpsert.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^8.49.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
